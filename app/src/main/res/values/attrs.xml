<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--定义一些波浪view的属性-->
    <declare-styleable name="WaveView">
        <attr name="waveCount" format="integer"></attr>
        <attr name="waveWidth" format="integer"></attr>
        <attr name="waveHeight" format="integer"></attr>
        <attr name="android:color" />
        <attr name="mode">
            <enum name="circle" value="-1"></enum>
            <enum name="triangle" value="-2"></enum>
        </attr>


    </declare-styleable>

</resources>